            <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="extract form fields" enabled="true">
              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="parameters"></stringProp>
              <stringProp name="filename"></stringProp>
              <stringProp name="script">import java.util.regex.Matcher;
import java.util.regex.Pattern;
import java.util.ArrayList;
import java.util.Random;

public String joinArrayListString(ArrayList r, String delimiter) {
	if(r == null || r.size() == 0 ){
		return &quot;&quot;;
	}
	StringBuffer sb = new StringBuffer();
	int i, len = r.size() - 1;
	for (i = 0; i &lt; len; i++){
		sb.append(r.get(i) + delimiter);
	}
	return sb.toString() + r.get(i);
}

// get the complete HTML markup of the signing page
var page = new String(data);

// prepare parsing out the field names
var r = Pattern.compile(&quot;name=\&quot;(petition_signing\\[(.*?)\\])\&quot;&quot;);
var m = r.matcher(page);

var body  = new ArrayList();
var names = new String[] {&quot;mariamuster&quot;, &quot;frauverbissenheit&quot;, &quot;cocochanel&quot;, &quot;jeandarc&quot;, &quot;mariecurie&quot;, &quot;cleopatra&quot;, &quot;Marilyn%20Monroe&quot;, &quot;Ada%20Lovelace&quot;};

//log.info(page);

while (m.find()) {
	var name = m.group(2);
	if (name.equals(&quot;id&quot;)) continue;

	var value = &quot;dummy-value&quot;;
	
	switch (name) {
		case &quot;fullname&quot;:
			idx = new Random().nextInt(names.length);
			value = names[idx];
			break;
			
		case &quot;email&quot;:
			value = &quot;policat-stresstest-&quot; + (new Random().nextInt(100000)) + &quot;-random-&quot; + (new Random().nextInt(100000)) + &quot;@mailinator-dummy.com&quot;;
			break;
			
		case &quot;country&quot;:
			value = &quot;DE&quot;;
			break;
			
		case &quot;title&quot;:
			value = &quot;female&quot;;
			break;
			
		case &quot;privacy&quot;:
			value = &quot;1&quot;;
			break;
			
		case &quot;subscribe&quot;:
			value = &quot;&quot;;
			break;
			
		case &quot;ref&quot;:
			value = &quot;&quot;;
			break;
	}
	
	body.add(m.group(1) + &quot;=&quot; + value);
}

vars.put(&quot;SIGN_PAYLOAD&quot;, joinArrayListString(body, &quot;&amp;&quot;));
</stringProp>
            </BeanShellPostProcessor>
